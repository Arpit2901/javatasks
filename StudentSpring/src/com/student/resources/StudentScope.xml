<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="studCollection" class="com.collection.bean.CollectionStu" scope="prototype" autowire="byName">
		<property name="rollno" value="105"></property>
        <property name="name" value="SHobhna"></property>
        <property name="salary" value="50000.0"></property>
        <property name="exam" ref="exam12"></property>
		<property name="names"><set>
		<value>Sharma</value>
		<value>khan</value>
		<value>Singh</value>
		</set>
		</property>
		<property name="Salaries"><list>
		<value>5000.0</value>
		<value>7000.0</value>
		<value>4000.0</value>
		</list>
		</property>
		<property name="NamesRecord"><map>
		<entry key="Naaam" value="Arpit"></entry> 
		<entry key="Name" value="Tushar"></entry>
		<entry key="Naam" value="Saurab"></entry>
		</map>
		</property>
		
	</bean>
	<bean id="exam12" class="com.collection.bean.CollectionExam" scope="prototype">
		<constructor-arg value="51" index="0"></constructor-arg>
		<constructor-arg value="Java" index="1"></constructor-arg>
		<constructor-arg value="100" index="2"></constructor-arg>
	</bean>
</beans>